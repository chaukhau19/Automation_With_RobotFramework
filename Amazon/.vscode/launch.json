{
    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch template",
            "request": "launch",
            "terminal": "integrated",
            "args": [
                "-d", "${workspaceFolder}/Log",
            ]
        },
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch .robot file",
            "request": "launch",
            "cwd": "^\"\\${workspaceFolder}\"",
            "target": "^\"\\${file}\"",
            "terminal": "integrated",
            "env": {},
            "args": [
                "-d", "${workspaceFolder}/Log",
            ]
        }
    ]
}